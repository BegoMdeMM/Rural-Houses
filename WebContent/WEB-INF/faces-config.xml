<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>seleccion</managed-bean-name>
		<managed-bean-class>modelo.Seleccion</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>setAvailability</managed-bean-name>
		<managed-bean-class>modelo.SetAvailability</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reserveBean</managed-bean-name>
		<managed-bean-class>modelo.ReserveBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>selectedHouse</property-name>
			<property-class>principal.RuralHouse</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>entrada</property-name>
			<property-class>java.util.Date</property-class>
			<value></value>
		</managed-property>
		<managed-property>
			<property-name>salida</property-name>
			<property-class>java.util.Date</property-class>
			<value></value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>modelo.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>desplegable</managed-bean-name>
		<managed-bean-class>modelo.Desplegable</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Seleccion.xhtml</display-name>
		<from-view-id>/Seleccion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>query</from-outcome>
			<to-view-id>/Deplegable.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Seleccion.xhtml</display-name>
		<from-view-id>/Seleccion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>set</from-outcome>
			<to-view-id>/SetAvalability.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SetAvalability.xhtml</display-name>
		<from-view-id>/SetAvalability.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Seleccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Deplegable.xhtml</display-name>
		<from-view-id>/Deplegable.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Seleccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Seleccion.xhtml</display-name>
		<from-view-id>/Seleccion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>res</from-outcome>
			<to-view-id>/SetReserve.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SetReserve.xhtml</display-name>
		<from-view-id>/SetReserve.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Seleccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Seleccion.xhtml</display-name>
		<from-view-id>/Seleccion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/Login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Login.xhtml</display-name>
		<from-view-id>/Login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>log</from-outcome>
			<to-view-id>/Seleccion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
